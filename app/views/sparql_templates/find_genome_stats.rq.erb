DEFINE sql:select-option "order"

<%= prefix[:stats] %>

SELECT DISTINCT ?taxonomy_id ?genome_size ?gene_num ?rrna_num ?trna_num ?ncrna_num
FROM <%= ontology[:stats] %>
WHERE {
  VALUES ?taxonomy_id { <%= taxonomies %>}
  ?taxonomy_id stats:sequence_length ?genome_size ;
               stats:gene ?gene_num ;
               stats:rrna ?rrna_num ;
               stats:trna ?trna_num ;
               stats:ncrna ?ncrna_num .
}
